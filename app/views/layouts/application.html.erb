<!DOCTYPE html>
<html>
<head>
  <title>Overtime</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>

  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>

<body>
<%= gflash %>
<%= js add_gritter(flash[:notice],:image => :notice, :title => "Overtime Application", :sticky => false) %>
<div class="container">
  <div class="row">
    <%= render 'shared/nav' %>
    <br/>
    <div class="col-md-12">
      <%= yield %>
    </div>
  </div>
</div>
</body>
</html>
